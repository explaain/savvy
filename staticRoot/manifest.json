{
  "name": "Savvy",
  "short_name": "Savvy",
  "description": "Smart team knowledge management",
  "version": "1.1.3",
  "manifest_version": 2,
  "icons": {
    "16": "static/chrome/images/icon16.png",
    "48": "static/chrome/images/icon48.png",
    "128": "static/chrome/images/icon128.png"
  },
  "content_scripts" : [{
    "matches" : [
      "*://*/*"
    ],
    "js" : [
      "static/chrome/contentScript.js"
    ]
  }],
  "background": {
    "scripts": [
      "static/chrome/js/firebase.js",
      "static/chrome/eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title" : "Savvy",
    "default_icon": {
      "48": "static/chrome/images/icon48.png"
    },
    "default_title": "Savvy"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://savvy-nlp--staging.herokuapp.com/ https://*.logrocket.io https://www.gstatic.com/ https://*.firebaseio.com https://*.intercom.io https://www.googleapis.com https://apis.google.com/ https://forget-me-not--staging.herokuapp.com https://savvy-api--live.herokuapp.com; object-src 'self'; child-src 'self' https://api.kloudless.com/ blob:; worker-src 'self' blob:; connect-src https://api.airshiphq.com/ https://*.googleapis.com https://savvy-nlp--staging.herokuapp.com https://api.kloudless.com/ https://*.logrocket.io https://*.intercom.io https://api.mixpanel.com https://*.algolia.net https://savvy-api--live.herokuapp.com;",
  "web_accessible_resources": [
    "newtab.html",
    "sidebar.html"
  ],
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "cookies",
    "declarativeContent",
    "identity",
    "management",
    "notifications",
    "proxy",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "oauth2": {
    "client_id": "704974264220-bjqgp1g3aljp865ehn2kd5s2og2qkuqf.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": ""
}
